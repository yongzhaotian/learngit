			sElseFee = DataConvert.toHTMLMoney(sElseFee);
		}
		if(sDunSum != null && !sDunSum.equals("") && !sDunSum.equals("&nbsp"))
		{
			sDunSum = DataConvert.toHTMLMoney(sDunSum);
		}
		//按年月日取出终结日期
		if(sMaturity != null && !sMaturity.equals(""))
		{
			sYear = sMaturity.substring(0,4);
			sMonth = sMaturity.substring(5,7);
			sDay = sMaturity.substring(8,10);
		}
		//获取系统当前日期
