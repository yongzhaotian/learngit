		{
			as_add("myiframe0");//新增记录
			setItemValue(0, 0, "ITEMNAME", getSerialNo("<%=sTableName%>", "ITEMNAME", ""));
			setItemValue(0, 0, "SCENEID", "<%=sSceneId%>");
			bIsInsert = true;
		}
	}
	</script>
