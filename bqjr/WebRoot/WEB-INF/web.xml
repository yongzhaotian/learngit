    </init-param>
    <init-param>
      <param-name>ExcludedURLs</param-name>
      <param-value>^/.*Logon.jsp</param-value>
    </init-param>
  </filter>
  <filter-mapping>
    <filter-name>RunFilter</filter-name>
    <url-pattern>*.jsp</url-pattern>
    <dispatcher>REQUEST</dispatcher>
    <dispatcher>FORWARD</dispatcher>
  </filter-mapping>
