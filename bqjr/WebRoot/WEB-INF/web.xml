  <filter>
    <filter-name>RunFilter</filter-name>
    <filter-class>com.amarsoft.awe.control.RunFilter</filter-class>
    <init-param>
      <param-name>RunTimeDebug</param-name>
      <param-value>true</param-value>
    </init-param>
    <init-param>
      <param-name>ShowFramePage</param-name>
      <param-value>true</param-value>
    </init-param>
