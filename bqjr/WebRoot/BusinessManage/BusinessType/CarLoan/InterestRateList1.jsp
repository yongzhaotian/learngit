    var bIsInsert = false; //标记DW是否处于“新增状态”

	//---------------------定义按钮事件------------------------------------
	
	function newRecord(){
		OpenPage("/BusinessManage/BusinessType/CarLoan/InterestRateInfo1.jsp","_self","");
	}
   
    function myDetail(){
    	var sInterestRateID =getItemValue(0,getRow(),"InterestRateID");
		if (typeof(sInterestRateID)=="undefined" || sInterestRateID.length==0){
			alert("请至少选择一条记录！");
			return;
		}
		AsControl.OpenView("/BusinessManage/BusinessType/CarLoan/InterestRateDetailInfo1.jsp","InterestRateID="+sInterestRateID,"right","");
    }
    
	function deleteRecord(){
		var sInterestRateID =getItemValue(0,getRow(),"InterestRateID");//获取删除记录的单元值
		if (typeof(sInterestRateID)=="undefined" || sInterestRateID.length==0){
			alert("请至少选择一条记录！");
			return;
		}
		
		if(confirm("您真的想删除该信息吗？")){
			as_del("myiframe0");
			as_save("myiframe0");  //如果单个删除，则要调用此语句
			 reloadSelf();
		}
	}
	
	</script>

